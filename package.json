{
  "name": "google-indexing-tool",
  "version": "1.0.0",
  "description": "A Bun-based tool for bulk Google Indexing API requests with a modern dark-themed UI",
  "main": "src/server.ts",
  "scripts": {
    "dev": "bun --hot src/server.ts",
    "start": "bun src/server.ts",
    "build": "bun build src/server.ts --outdir=dist",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "type-check": "bun --check src/server.ts"
  },
  "keywords": [
    "google",
    "indexing",
    "api",
    "bun",
    "seo",
    "bulk-indexing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^131.0.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@types/node": "^20.11.0",
    "bun-types": "^1.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
} 